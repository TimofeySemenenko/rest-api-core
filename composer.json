{
    "name": "evgeny-l/rest-api-core",
    "description": "REST API Core. Handles REST API requests and responses.",
    "keywords": ["laravel", "rest", "api", "api-core"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Leksunin",
            "email": "e.leksunin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "league/fractal": "^0.16.0",
        "laravel/framework": ">5.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "EvgenyL\\RestAPICore\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EvgenyL\\RestAPICore\\RestAPICoreServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
